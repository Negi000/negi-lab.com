  "probabilityFormulas": {
    "perTile": {
      "formula": "1 / remainingTiles",
      "description": "各未開封マスの確率 = 1 / 残りマス数"
    },
    "expectedTrials": {
      "formula": "(remainingTiles + 1) / 2",
      "description": "期待試行回数 = (残りマス数 + 1) / 2"
    },
    "medianTrials": {
      "formula": "Math.ceil(remainingTiles / 2)",
      "description": "50%確率で発見できる試行回数 = 残りマス数 / 2 (切り上げ)"
    },
    "cumulativeProbability": {
      "formula": "trialsCount / remainingTiles",
      "description": "n回試行後の累積発見確率 = n / 残りマス数"
    }
  },
